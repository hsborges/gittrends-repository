{
  "name": "locations-research",
  "version": "0.0.1",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist && yarn tsc",
    "prettier": "prettier --write --config ../../.prettierrc.yml --ignore-path ../../.prettierignore  '**/*.{ts,tsx}'",
    "dev:export-locations": "ts-node-dev -r tsconfig-paths/register -r @gittrends/env-config -T export-locations.ts",
    "dev:update-locations": "ts-node-dev -r tsconfig-paths/register -r @gittrends/env-config -T update-locations.ts",
    "dev:export-rankings": "ts-node-dev -r tsconfig-paths/register -r @gittrends/env-config -T export-rankings.ts",
    "export-locations": "node -r @gittrends/env-config dist/export-locations.js",
    "export-rankings": "node -r @gittrends/env-config dist/export-rankings.js",
    "update-locations": "node -r @gittrends/env-config dist/update-locations.js"
  },
  "dependencies": {
    "@gittrends/database-config": "file:../../packages/database-config",
    "commander": "^8.1.0",
    "consola": "^2.15.3",
    "fast-csv": "^4.3.6",
    "lodash": "^4.17.21",
    "mkdirp": "^1.0.4"
  },
  "devDependencies": {
    "@gittrends/env-config": "file:../../packages/env-config",
    "@types/mkdirp": "^1.0.2",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.10.1"
  }
}
