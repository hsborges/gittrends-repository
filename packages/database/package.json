{
  "name": "@gittrends/database",
  "version": "4.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "rm -rf dist && yarn tsc",
    "test": "jest",
    "migrate:create": "sh -c 'ts-node node_modules/.bin/migrate-mongo create $0 -f migrate-mongo.ts' ",
    "migrate:status": "ts-node -r @gittrends/env-config node_modules/.bin/migrate-mongo status -f migrate-mongo.ts",
    "migrate:undo-last": "ts-node -r @gittrends/env-config node_modules/.bin/migrate-mongo down -f migrate-mongo.ts && npm run migrate:status",
    "migrate": "ts-node -r @gittrends/env-config node_modules/.bin/migrate-mongo up -f migrate-mongo.ts",
    "prettier": "prettier --write --config ../../.prettierrc.yml --ignore-path ../../.prettierignore  '**/*.ts'"
  },
  "dependencies": {
    "class-transformer": "0.4.0",
    "class-validator": "^0.13.2",
    "es6-shim": "^0.35.6",
    "lodash": "^4.17.21",
    "migrate-mongo": "^8.2.3",
    "mongodb": "4.2.0",
    "reflect-metadata": "^0.1.13",
    "stack-trace": "0.0.10",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@gittrends/env-config": "link:../env-config",
    "@shelf/jest-mongodb": "^2.1.0",
    "@types/es6-shim": "^0.31.42",
    "@types/jest": "^27.0.3",
    "@types/lodash": "^4.14.177",
    "@types/stack-trace": "^0.0.29",
    "@types/uuid": "^8.3.3",
    "jest": "^27.3.1",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0"
  }
}
