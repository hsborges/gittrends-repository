{
  "name": "@gittrends/service",
  "version": "3.0.0",
  "private": true,
  "license": "MIT",
  "author": "Hudson Silva Borges <hudsonsilbor@gmail.com>",
  "scripts": {
    "build": "yarn clean && yarn tsc",
    "clean": "rm -rf dist",
    "proxy-server": "github-proxy-server --api graphql --tokens ../../data/tokens.txt",
    "add-repositories": "node -r @gittrends/env-config dist/add-repositories.js",
    "schedule": "node -r @gittrends/env-config dist/scheduler.js",
    "update": "node -r @gittrends/env-config --expose-gc dist/updater.js",
    "repos-stats": "node -r @gittrends/env-config dist/extra/repos-stats.js",
    "dev:schedule": "ts-node -r tsconfig-paths/register -r @gittrends/env-config -T scheduler.ts",
    "dev:add-repositories": "ts-node -r tsconfig-paths/register -r @gittrends/env-config -T add-repositories.ts",
    "dev:update": "ts-node -r tsconfig-paths/register -r @gittrends/env-config -T updater.ts",
    "dev:queue-board": "ts-node -r tsconfig-paths/register -r @gittrends/env-config -T extra/queue-board.ts",
    "dev:repos-stats": "ts-node -r tsconfig-paths/register -r @gittrends/env-config -T extra/repos-stats.ts"
  },
  "dependencies": {
    "@gittrends/database-config": "file:../database-config",
    "axios": "^0.21.1",
    "bluebird": "^3.7.2",
    "bull-board": "^1.2.0",
    "bullmq": "^1.14.0",
    "chalk": "^4.1.0",
    "commander": "^6.2.1",
    "consola": "^2.15.0",
    "dayjs": "^1.10.3",
    "express": "^4.17.1",
    "ioredis": "^4.19.4",
    "js-yaml": "^4.0.0",
    "lodash": "^4.17.20",
    "numeral": "^2.0.6",
    "object-hash": "^2.1.1",
    "retry": "^0.12.0",
    "table": "^6.0.7",
    "transitory": "^2.1.1",
    "typescript-collections": "^1.3.3",
    "user-agents": "^1.0.559"
  },
  "devDependencies": {
    "@gittrends/env-config": "file:../env-config",
    "@hsborges/github-proxy-server": "^3.0.11",
    "@types/bluebird": "^3.5.33",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.11",
    "@types/ioredis": "^4.17.11",
    "@types/numeral": "^0.0.29",
    "@types/object-hash": "^1.3.4",
    "@types/retry": "^0.12.0",
    "@types/table": "^6.0.0",
    "@types/user-agents": "^1.0.0",
    "core-util-is": "^1.0.2",
    "pm2": "^4.5.1",
    "ts-node": "^9.1.1"
  },
  "config": {
    "resources": [
      "repository",
      "stargazers",
      "watchers",
      "tags",
      "releases",
      "issues",
      "pull_requests",
      "dependencies",
      "users"
    ]
  }
}
