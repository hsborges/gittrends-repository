{
  "name": "@gittrends/service",
  "version": "4.0.0",
  "private": true,
  "license": "MIT",
  "author": "Hudson Silva Borges <hudsonsilbor@gmail.com>",
  "scripts": {
    "build": "rm -rf dist && yarn tsc",
    "test": "node -r @gittrends/env-config node_modules/.bin/jest",
    "prettier": "prettier --write --config ../../.prettierrc.yml --ignore-path ../../.prettierignore  '**/*.ts'",
    "proxy-server": "github-proxy-server --api graphql --tokens ../../tokens.txt",
    "add-repositories": "node -r @gittrends/env-config dist/add-repositories.js",
    "schedule": "node -r @gittrends/env-config dist/scheduler.js",
    "update": "node --expose-gc -r @gittrends/env-config dist/updater.js",
    "repos-stats": "node -r @gittrends/env-config dist/extra/repos-stats.js",
    "queue-board": "node -r @gittrends/env-config dist/extra/queue-board.js",
    "dev:schedule": "ts-node -r tsconfig-paths/register -r @gittrends/env-config -T scheduler.ts",
    "dev:add-repositories": "ts-node -r tsconfig-paths/register -r @gittrends/env-config -T add-repositories.ts",
    "dev:update": "ts-node -r tsconfig-paths/register -r @gittrends/env-config -T updater.ts",
    "dev:queue-board": "ts-node -r tsconfig-paths/register -r @gittrends/env-config -T extra/queue-board.ts",
    "dev:repos-stats": "ts-node -r tsconfig-paths/register -r @gittrends/env-config -T extra/repos-stats.ts",
    "clinic-doctor": "clinic doctor -- node -r @gittrends/env-config dist/updater.js",
    "clinic-bubbleprof": "clinic bubbleprof -- node -r @gittrends/env-config dist/updater.js",
    "clinic-flame": "clinic flame -- node -r @gittrends/env-config dist/updater.js",
    "pm2": "node -r @gittrends/env-config ./node_modules/.bin/pm2",
    "pm2-runtime": "node -r @gittrends/env-config ./node_modules/.bin/pm2-runtime"
  },
  "dependencies": {
    "@gittrends/database-config": "link:../database-config",
    "axios": "^0.24.0",
    "bee-queue": "^1.4.0",
    "bluebird": "^3.7.2",
    "bull-arena": "^3.29.3",
    "chalk": "^4.1.2",
    "commander": "^8.3.0",
    "consola": "^2.15.3",
    "cron": "^1.8.2",
    "dayjs": "^1.10.7",
    "debug": "^4.3.2",
    "express": "^4.17.1",
    "graphql-query-compress": "^1.2.4",
    "ioredis": "^4.28.1",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "node-object-hash": "^2.3.10",
    "numeral": "^2.0.6",
    "promise-retry": "^2.0.1",
    "redis-lru": "^0.6.0",
    "table": "^6.7.3",
    "user-agents": "^1.0.842"
  },
  "devDependencies": {
    "@gittrends-app/github-proxy-server": "gittrends-app/github-proxy-server",
    "@gittrends/env-config": "link:../env-config",
    "@types/bluebird": "^3.5.36",
    "@types/bull-arena": "^3.0.4",
    "@types/cron": "^1.7.3",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/ioredis": "^4.28.1",
    "@types/nock": "^11.1.0",
    "@types/numeral": "^2.0.2",
    "@types/promise-retry": "^1.1.3",
    "@types/user-agents": "^1.0.2",
    "core-util-is": "^1.0.3",
    "get-port": "^6.0.0",
    "jest": "^27.3.1",
    "nock": "^13.2.1",
    "pm2": "^5.1.2",
    "ts-jest": "^27.0.7"
  },
  "config": {
    "resources": [
      "repository",
      "stargazers",
      "watchers",
      "tags",
      "releases",
      "issues",
      "pull_requests",
      "dependencies",
      "users"
    ]
  }
}
